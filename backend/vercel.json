{
  "version": 2,
  "projects": {
    "name": "moodswings",
    "functions": {
      "api/**/*.js": { "runtime": "nodejs18.x" }
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/moodswings-app/$1"
    }
  ]
}
